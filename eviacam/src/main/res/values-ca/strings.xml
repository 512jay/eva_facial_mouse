    <string name="settings">Configuració</string>
    <string name="wiz_set_keyboard">
        <![CDATA[1. Obrir: \'Configuració > Idioma i introducció de text\']]>
        \n2. Activar \'Teclat EVA\'
        \n3. Tocar \'Predeterminat\'
        \n4. Escollir \'Teclat EVA\'
    </string>
    <string name="wiz_set_keyboard_lollipop">
        <![CDATA[1. Obrir: \'Configuració > Idioma i introducció de text > Teclat actual > TRIA ELS TECLATS\']]>\n
        \n2. Activar \'Teclat EVA\'
        \n3. Enrere
        \n4. Tocar \'Teclat actual\'
        \n5. Escollir \'Teclat EVA\'
    </string>
