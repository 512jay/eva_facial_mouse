    <string name="settings">Ajustes</string>
    <string name="wiz_set_keyboard">
        <![CDATA[1. Abrir: \'Ajustes > Idioma e introducción de texto\']]>
        \n2. Activar \'Teclado EVA\'
        \n3. Tocar \'Predeterminado\'
        \n4. Escoger \'Teclado EVA\'
    </string>
    <string name="wiz_set_keyboard_lollipop">
        <![CDATA[1. Abrir: \'Ajustes > Idioma e introducción de texto > Teclado actual > ELEGIR TECLADOS\']]>
        \n2. Activar \'Teclado EVA\'
        \n3. Atrás
        \n4. Tocar \'Teclado actual\'
        \n5. Escoger \'Teclado EVA\'
    </string>
